{
  "name": "dartegnians-portfolio",
  "version": "1.4.7",
  "scripts": {
    "ng": "ng",
    "start": "gulp compress && ng serve",
    "prebuild": "gulp compress",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run dartegnians-portfolio:serve-ssr",
    "serve:ssr": "node dist/dartegnians-portfolio/server/main.js",
    "build:ssr": "ng build --configuration production && ng run dartegnians-portfolio:server",
    "prerender": "gulp compress && ng run dartegnians-portfolio:prerender",
    "vercel-build": "npm run build:ssr",
    "scully-build": "ng build --configuration production && npx scully --noPrompt --project dartegnians-portfolio --scanRoutes"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.2",
    "@angular/common": "^17.0.2",
    "@angular/compiler": "^17.0.2",
    "@angular/core": "^17.0.2",
    "@angular/forms": "^17.0.5",
    "@angular/platform-browser": "^17.0.2",
    "@angular/platform-browser-dynamic": "^17.0.5",
    "@angular/platform-server": "^17.0.5",
    "@angular/router": "^17.0.5",
    "@angular/service-worker": "^17.0.5",
    "@angular/ssr": "^17.0.5",
    "@material/material-color-utilities": "^0.2.7",
    "bootstrap": "^5.3.2",
    "colorthief": "^2.4.0",
    "express": "^4.18.2",
    "idb": "^7.1.1",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.5",
    "@angular/cli": "^17.0.5",
    "@angular/compiler-cli": "^17.0.5",
    "@types/express": "^4.17.21",
    "@types/jasmine": "^4.6.4",
    "@types/node": "^20.10.2",
    "gulp": "^4.0.2",
    "gulp-sharp-responsive": "^0.4.1",
    "jasmine-core": "~4.1.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "~5.2.2"
  }
}
